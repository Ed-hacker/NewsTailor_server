INSERT INTO news_rankings (id, section_id, press, "rank", title, url, body, collected_at) VALUES
(2, 105, '중앙일보', 2, '삼성전자, 3나노 GAA 공정 양산 돌입', 'https://news.naver.com/main/read.naver?mode=LSD&mid=shm&sid1=105&oid=025&aid=0002001', '삼성전자가 차세대 반도체 공정인 3나노 GAA(Gate-All-Around) 기술의 양산을 시작했습니다. 이는 TSMC와의 기술 경쟁에서 우위를 점하기 위한 전략입니다.', DATEADD('HOUR', -2, CURRENT_TIMESTAMP));

INSERT INTO news_rankings (id, section_id, press, "rank", title, url, body, collected_at) VALUES
(3, 101, '한국경제', 1, '한국은행, 기준금리 동결 결정', 'https://news.naver.com/main/read.naver?mode=LSD&mid=shm&sid1=101&oid=015&aid=0003001', '한국은행 금융통화위원회가 기준금리를 현행 3.5%로 동결하기로 결정했습니다. 인플레이션 압력과 경기 둔화 우려를 종합적으로 고려한 결과입니다.', DATEADD('HOUR', -3, CURRENT_TIMESTAMP));

INSERT INTO news_rankings (id, section_id, press, "rank", title, url, body, collected_at) VALUES
(4, 100, '연합뉴스', 1, '국회, 민생법안 처리 합의', 'https://news.naver.com/main/read.naver?mode=LSD&mid=shm&sid1=100&oid=001&aid=0004001', '여야가 민생 관련 법안 처리에 합의하며 정치적 대립을 일시적으로 봉합했습니다. 소상공인 지원법과 청년 일자리 창출법이 포함됩니다.', DATEADD('HOUR', -4, CURRENT_TIMESTAMP));

INSERT INTO news_rankings (id, section_id, press, "rank", title, url, body, collected_at) VALUES
