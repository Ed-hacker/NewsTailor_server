INSERT INTO news (id, title, url, content, category, published_at) VALUES
(1, '국회, 2025년 예산안 통과', 'https://news.example.com/politics/1', '국회는 오늘 본회의를 열어 2025년도 예산안을 통과시켰습니다. 총 예산 규모는 전년 대비 5% 증가한 600조원입니다.', '정치', DATEADD('DAY', -1, CURRENT_TIMESTAMP));

INSERT INTO news (id, title, url, content, category, published_at) VALUES
(2, '코스피, 3000선 돌파', 'https://news.example.com/economy/1', '코스피 지수가 3000선을 돌파하며 연중 최고치를 기록했습니다. 반도체 및 자동차 업종이 강세를 보였습니다.', '경제', DATEADD('HOUR', -2, CURRENT_TIMESTAMP));

INSERT INTO news (id, title, url, content, category, published_at) VALUES
(3, 'AI 챗봇, 의료 진단 정확도 95% 달성', 'https://news.example.com/tech/1', '최신 AI 챗봇이 의료 진단에서 95%의 정확도를 달성하며 의료 분야의 혁신을 이끌고 있습니다.', 'IT/과학', DATEADD('HOUR', -3, CURRENT_TIMESTAMP));

INSERT INTO news (id, title, url, content, category, published_at) VALUES
(4, '서울시, 청년 주거 지원 확대', 'https://news.example.com/society/1', '서울시가 청년들의 주거 안정을 위해 월세 지원금을 기존 20만원에서 30만원으로 확대한다고 발표했습니다.', '사회', DATEADD('HOUR', -5, CURRENT_TIMESTAMP));

INSERT INTO news (id, title, url, content, category, published_at) VALUES
(5, '메타버스 콘서트, 10만명 동시 접속', 'https://news.example.com/culture/1', '국내 유명 가수의 메타버스 콘서트가 10만명의 동시 접속자를 기록하며 새로운 공연 문화를 선보였습니다.', '생활/문화', DATEADD('HOUR', -6, CURRENT_TIMESTAMP));

INSERT INTO news (id, title, url, content, category, published_at) VALUES
(6, 'UN, 기후변화 대응 긴급 회의', 'https://news.example.com/world/1', 'UN은 기후변화 대응을 위한 긴급 회의를 소집하고 각국의 탄소 배출 감축 목표를 상향 조정할 것을 촉구했습니다.', '세계', DATEADD('HOUR', -8, CURRENT_TIMESTAMP));

INSERT INTO news (id, title, url, content, category, published_at) VALUES
(7, '양자컴퓨터, 슈퍼컴퓨터 성능 초과', 'https://news.example.com/tech/2', '구글이 개발한 양자컴퓨터가 기존 슈퍼컴퓨터보다 1000배 빠른 연산 속도를 달성했습니다.', 'IT/과학', DATEADD('HOUR', -10, CURRENT_TIMESTAMP));

INSERT INTO news (id, title, url, content, category, published_at) VALUES
(8, '반도체 수출, 역대 최고치 경신', 'https://news.example.com/economy/2', '올해 반도체 수출액이 1500억 달러를 넘어서며 역대 최고치를 경신할 것으로 전망됩니다.', '경제', DATEADD('HOUR', -12, CURRENT_TIMESTAMP));

INSERT INTO news (id, title, url, content, category, published_at) VALUES
(9, '지방선거, 투표율 70% 돌파', 'https://news.example.com/politics/2', '이번 지방선거 투표율이 70%를 돌파하며 높은 시민 참여율을 보였습니다.', '정치', DATEADD('DAY', -1, CURRENT_TIMESTAMP));

INSERT INTO news (id, title, url, content, category, published_at) VALUES
(10, '전국 미세먼지 좋음 수준', 'https://news.example.com/society/2', '전국 대부분 지역의 미세먼지 농도가 좋음 수준을 유지하고 있어 야외 활동하기 좋은 날씨입니다.', '사회', DATEADD('HOUR', -1, CURRENT_TIMESTAMP));

